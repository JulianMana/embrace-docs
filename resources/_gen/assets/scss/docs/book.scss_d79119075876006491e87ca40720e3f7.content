.book-home {
  padding: 1rem; }

aside nav,
.book-page,
.book-header aside,
.markdown {
  transition: 0.2s ease-in-out;
  transition-property: transform, margin, opacity, visibility;
  will-change: transform, margin, opacity; }

@media screen and (max-width: 56rem) {
  .book-menu {
    visibility: hidden;
